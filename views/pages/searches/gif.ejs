<!DOCTYPE html>
<html lang="en">

<%- include('../partials/head') %>


<body>
  <%- include('../partials/header') %>


  <main>
    <%- include('../partials/search') %>
    <div class="newMain">
      <% meme.forEach (data => { %>
      <section>
        <li>
          <div class="container">
            <form name="data" method="post" action="/save">
              <img src='<%= data.url%>' alt="Avatar" class="image">
              <input type="hidden" name="data" value="<%=data.url%>">
              <button id="butt" type="submit">Save This Gif</button>
            </form>
          </div>
        </li>

      </section>

      <% }); %>
    </div>


  </main>
  <%- include('../partials/footer') %>
</body>

</html>